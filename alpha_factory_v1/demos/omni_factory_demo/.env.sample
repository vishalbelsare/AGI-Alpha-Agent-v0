# Ledger path (sqlite3 file). Created automatically if missing.
OMNI_LEDGER=./omni_ledger.sqlite
LEDGER_PATH=${OMNI_LEDGER}

# Metrics exporter & dashboard ports
PROM_PORT=9137
DASH_SERVER_PORT=8050

# Simulation parameters
OMNI_SUCCESS_THRESHOLD=0.95
OMNI_MAX_SIM_MINUTES=240
OMNI_MICRO_CURRICULUM=auto
OMNI_AGIALPHA_SUPPLY=10000000

# Optional cloud API keys (enables LLM & toolkit extras)
OPENAI_API_KEY=
GOOGLE_ADK_KEY=
A2A_API_KEY=
